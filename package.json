{
  "name": "iyikuyoro",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && ng build --prod",
    "clean": "rm -rf dist",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "start": "node server.js",
    "dev": "ng serve --port 4100",
    "test": "ng test --watch=false --code-coverage",
    "test:watch": "npm run lint && ng test --watch=true --code-coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.7",
    "@angular/cdk": "^7.3.3",
    "@angular/common": "^7.2.5",
    "@angular/compiler": "^7.2.5",
    "@angular/core": "^7.2.5",
    "@angular/forms": "^7.2.5",
    "@angular/http": "^7.2.5",
    "@angular/material": "^7.3.3",
    "@angular/platform-browser": "^7.2.5",
    "@angular/platform-browser-dynamic": "^7.2.5",
    "@angular/router": "^7.2.5",
    "@cloudinary/angular-5.x": "^1.0.2",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "cloudinary-core": "^2.6.2",
    "core-js": "^2.4.1",
    "express": "^4.16.4",
    "ngx-quill": "^5.1.0",
    "quill": "^1.3.6",
    "rxjs": "^6.4.0",
    "rxjs-compat": "^6.0.0-rc.0",
    "time_past": "^2.0.1",
    "tslib": "^1.9.0",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "^7.3.2",
    "@angular/compiler-cli": "^7.2.5",
    "@angular/language-service": "^7.2.5",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "commitplease": "^3.2.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "^3.2.4"
  },
  "commitplease": {
    "style": "angular",
    "types": [
      "feat",
      "fix",
      "docs",
      "style",
      "refactor",
      "pref",
      "test",
      "chore"
    ],
    "scope": "\\S+.*"
  },
  "nyc": {
    "extensions": [
      ".ts"
    ],
    "exclude": [
      "**/*.js",
      "**/*.spec.ts",
      "**/tests.*.ts"
    ]
  }
}
